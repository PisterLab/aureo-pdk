drDefineDisplay(
;( DisplayName )
 ( display       )
)

drDefineColor(
; ( DisplayName  ColorName  Red  Green  Blue  Blink )
  ( display      white      255  255    255         )
  ( display      whiteB     255  255    255   t     )
  ( display      black      0    0      0           )
  ( display      blackB     0    0      0     t     )
  ( display      red        255  0      0           )
  ( display      redB       255  0      0     t     )
  ( display      green      0    255    0           )
  ( display      greenB     0    255    0     t     )
  ( display      blue       0    0      255         )
  ( display      blueB      0    0      255   t     )
  ( display      yellow     255  255    0           )
  ( display      yellowB    255  255    0     t     )
  ( display      magenta    255  0      255         )
  ( display      magentaB   255  0      255   t     )
  ( display      cyan       0    255    255         )
  ( display      cyanB      0    255    255   t     )
  ( display      orange     255  128    0           )
  ( display      orangeB    255  128    0     t     )
  ( display      purple     128  0      255         )
  ( display      purpleB    128  0      255   t     )
  ( display      gray       128  128    128         )
  ( display      grayB      128  128    128   t     )
